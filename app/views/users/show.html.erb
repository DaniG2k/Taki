<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

<ol>
<% @users.each do |user| %>
	<li>
		<strong><%= user.first_name %> <%= user.last_name %></strong>
		<ul>
			<li><%= image_tag user.avatar_url.to_s %></li>
			<li><%= user.email %></li>
			<li><%= user.gender %></li>
			<li><%= user.birthday %></li>
			<li>Is tutor: <%= user.tutor %></li>
			<li>Is student: <%= user.student %></li>
		</ul>
	</li>
<% end %>
</ol>
<%= link_to "Home", root_path %>