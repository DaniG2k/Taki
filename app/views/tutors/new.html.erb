<% title 'New Tutor'%>
<h1>New Tutor</h1>

<%= render "errors" %>

<%= form_for @tutor do |f| %>
  <%= f.label :description, 'Description' %><br />
	<%= f.text_area :description %><br />
	
	<%= f.label :rate, 'Rate' %><br />
	<%= f.text_field :rate %><br />
	
	<%= country_select(:tutor, :country) %><br />
  
  <%= f.submit "Submit" %>
<% end %>