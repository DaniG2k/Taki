<% title 'Edit Tutor'%>
<h1>Edit Tutor</h1>

<%= render "errors" %>

<%= form_for @tutor do |f| %>
  <%= f.label :description, t('activerecord.attributes.tutor.description') %><br />
	<%= f.text_area :description %><br />
	
	<%= f.label :rate, t('activerecord.attributes.tutor.rate') %><br />
	<%= f.text_field :rate %><br />
	
	<%= country_select(:tutor, :country) %><br />
  
  <%= f.submit "Submit" %>
<% end %>
<p>
	<%= link_to 'Back', tutors_path %>
</p>