<% title 'Edit Tutor'%>
<%= flash[:error] %>
<%= form_for @tutor do |f| %>
  <%= f.label :description, 'Description' %><br />
	<%= f.text_area :description, maxlength: 1000 %><br />
	
	<%= f.label :rate, 'Rate' %><br />
	<%= f.text_field :rate %><br />
	
	<%= country_select(:tutor, :country) %><br />
  
  <%= f.submit "Submit" %>
<% end %>
